{
"compilerOptions": {
"target": "ES2021",
"lib": [
"ES2021"
],
"types": [
    "@types/node"
],
"module": "ES2022",
"sourceMap": true,
"declaration": true,
"newLine": "lf",
"outDir": "./dist/",
"esModuleInterop": true,
"forceConsistentCasingInFileNames": true,
"strict": true,
"noUncheckedIndexedAccess": true,
"noImplicitOverride": true,
"moduleResolution": "node",
"skipLibCheck": true,
"allowJs": true,
"checkJs": false
},
"include": [
"src/**/*"
]
}